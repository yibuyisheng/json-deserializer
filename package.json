{
  "name": "json-deserializer",
  "version": "0.0.5",
  "description": "通过配置 schema 将 JSON 数据转换成指定的类型和格式。",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "publish-npm": "tsc && npm publish && npm run tag",
    "tag": "node tool/tag.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yibuyisheng/json-deserializer.git"
  },
  "keywords": [
    "JSON",
    "deserializer",
    "validator"
  ],
  "author": "yibuyisheng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yibuyisheng/json-deserializer/issues"
  },
  "homepage": "https://github.com/yibuyisheng/json-deserializer#readme",
  "typings": "types/index.d.ts",
  "peerDependencies": {
    "moment": "^2.21.0",
    "veui": "^1.0.0-alpha.9"
  },
  "sideEffects": false,
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(moment|core-js|babel-runtime|regenerator-runtime|lodash)/"
    ],
    "collectCoverage": false,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "testPathIgnorePatterns": [
      "/(node_modules|lib|coverage|types)/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^22.4.3",
    "moment": "^2.21.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1",
    "veui": "^1.0.0-alpha.9"
  },
  "dependencies": {}
}
